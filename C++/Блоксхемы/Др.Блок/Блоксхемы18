#include <iostream>
#include <math.h>
using namespace std;

int main()
{
    float a, b, c;
    cout<<"\nEnter a value a - "; cin>>a;
    cout<<"\nEnter a value b - "; cin>>b;
    cout<<"\nEnter a value c - "; cin>>c;
    if(a>c)
    {
        cout<<"\nThe cat doesn't fit";
    }
    else
    {
        if(c<b)
        {
            cout<<"\nThe cat fits";
        }
        else
        {
           cout<<"\nThe cat doesn't fit"; 
        }
    }
    return 0;
}
