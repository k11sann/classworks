// Субботин ИС 22.9-П 29.10.2024 СР6
#include <iostream>
#include <math.h>
using namespace std;

int main() {
    int grades[22];
    int count = 0;
    
    cout<<"\nВведите 22 значения для каждого ученика";
    
    for (int i=0; i<22; i++){
        cin >> grades[i];
        if (grades[i]==4||grades[i]==5){
            count++;
        }
    }
    
    
    cout<<"\nКоличество учеников с 4 и 5 : "<<count;
    return 0;
}
